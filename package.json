{
  "name": "gym-webapp-fullstack",
  "version": "1.0.0",
  "description": "Backend + Frontend merged workspace for gym webapp and AI frontend",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "npm run build:backend && npm run build:frontend",
    "start:backend": "node server/dist/index.js",
    "dev:backend": "ts-node-dev --respawn --transpile-only --project server/tsconfig.json server/src/index.ts",
    "build:backend": "tsc -p server/tsconfig.json",
    "dev:frontend": "next dev",
    "build:frontend": "next build",
    "start:frontend": "next start",
    "db:create": "cd server && sequelize-cli db:create",
    "db:drop": "cd server && sequelize-cli db:drop",
    "migrate": "cd server && sequelize-cli db:migrate",
    "migrate:undo": "cd server && sequelize-cli db:migrate:undo",
    "migrate:undo:all": "cd server && sequelize-cli db:migrate:undo:all",
    "migrate:status": "cd server && sequelize-cli db:migrate:status",
    "seed": "cd server && sequelize-cli db:seed:all",
    "seed:undo": "cd server && sequelize-cli db:seed:undo:all",
    "lint": "next lint"
  },
  "author": "Sarthak",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.2",
    "nodemailer": "^7.0.5",
    "sequelize": "^6.37.7",
    "validator": "^13.15.15",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.540.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.62.0",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^3.0.4",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
