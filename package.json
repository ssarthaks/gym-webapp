    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^7.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.5.6",
    "sequelize-cli": "^6.6.2"
  }
}
